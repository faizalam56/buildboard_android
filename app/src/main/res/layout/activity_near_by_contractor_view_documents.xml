            android:layout_marginEnd="@dimen/_10sdp"
            android:src="@drawable/carpenter"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view_two"
            android:layout_marginRight="@dimen/_10sdp" />
