<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".modules.home.modules.marketplace.NearByContractorViewDocumentsActivity">

        <include
            android:id="@+id/toolbar"
            layout="@layout/toolbar" />

        <com.buildboard.customviews.BuildBoardTextView
            android:id="@+id/text_business_licensing"
            style="@style/textSize_small"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:gravity="start"
            android:text="@string/business_licensing"
            android:textColor="@android:color/black"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/toolbar"
            app:typeface="robotoLight" />

        <com.buildboard.customviews.BuildBoardTextView
            android:id="@+id/text_state"
            style="@style/textSize_smaller"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:text="@string/state"
            android:textColor="@color/colorGreen"
            android:gravity="start"
            app:layout_constraintTop_toBottomOf="@+id/text_business_licensing"
            app:typeface="robotoRegular" />

        <com.buildboard.customviews.BuildBoardTextView
            android:id="@+id/text_state_name"
            style="@style/textSize_smaller"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_5sdp"
            android:text="@string/state"
            android:textColor="@android:color/black"
            android:gravity="start"
            app:layout_constraintTop_toBottomOf="@+id/text_state"
            app:typeface="robotoRegular" />

        <View
            android:id="@+id/view"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_10sdp"
            app:layout_constraintTop_toBottomOf="@+id/text_state_name"
            android:background="@drawable/divider_drawable" />

        <com.buildboard.customviews.BuildBoardTextView
